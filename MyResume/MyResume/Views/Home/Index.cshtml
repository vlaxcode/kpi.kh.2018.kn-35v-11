                                        </div>
                                    </div>
                                </div>
                                <!-- end message -->
                                <!-- form element -->
                                <div class="row">
                                    @using (Html.BeginForm("ResponseForm", "Home", FormMethod.Post))
                                    {
                                        <div class="col-md-6 form-group">
                                            @Html.TextBoxFor(x => x.Name, new { @placeholder = "Name", @class = "inputStyle form-control" })
                                        </div>
                                        <div class="col-md-6 form-group">
                                            @Html.TextBoxFor(x => x.Email, new { @placeholder = "Email", @class = "inputStyle form-control" })
                                        </div>
                                        <div class="col-md-6 form-group">
                                            @Html.TextBoxFor(x => x.Phone, new { @placeholder = "Phone", @class = "inputStyle form-control" })
                                        </div>
                                        <div class="col-md-6 form-group">
                                            @Html.TextBoxFor(x => x.Subject, new { @placeholder = "Subject", @class = "inputStyle form-control" })
                                        </div>
                                        <div class="col-12 form-group">
                                            @Html.TextAreaFor(x => x.Message,new { @placeholder = "Message", @class = "inputStyle form-control" })
                                        </div>
                                        <div class="col-12">
                                            <input name="submit" type="submit" class="btn btn-success" value="Send Message">
                                        </div>
                                    }
                                </div>
                                <!-- end form element -->
                            </form>
                        </div>
